  "authors": [
    "coderbot16",
    "NeusFear",
    "TerraformersMC"
  ],
  "contributors": [
    "Valoeghese",
    "SuperCoder79",
    "Prospector"
  ],
  "contact": {
    "sources": "https://github.com/TerraformersMC/Terrestria",
    "issues": "https://github.com/TerraformersMC/Terrestria/issues"
